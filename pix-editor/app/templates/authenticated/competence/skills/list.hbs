<div class="main-title skill-header">
  <h1 class="ui header">
    <div class="ui right floated menu">
      <button class="ui button icon item" {{on "click" this.close}} type="button"><i class="icon window close"></i></button>
    </div>
    Versions de {{this.skillName}}
  </h1>
</div>
<div class="ui attached segment competence">
  <List::Skills @list={{this.model.sortedSkills}}/>
</div>
<div class="ui borderless bottom attached labelled icon menu">
  {{#if this.mayCreateSkill}}
    <button class="ui button right item" {{on "click" this.newSkillVersion}} type="button">
      <i class="plus square outline icon"></i> Nouvelle Version
    </button>
  {{/if}}
</div>