<EmberTable as |t|>
  <t.head
          @columns={{this.headers}}
          @widthConstraint={{'eq-container'}}
          @resizeMode={{'fluid'}}
          @onUpdateSorts={{this.sortBy}}
          @sortFunction={{this.sort}}
          @sorts={{this.sorts}}/>
  <t.body
          @rows={{@list}}
          @rowSelectionMode={{'single'}}
          @checkboxSelectionMode={{'none'}}
           as |b|>
    <b.row data-test-skillLog as |r|>
      <r.cell as |value|>
        {{#if r.columnValue.skill}}
          <EventsLog::CellSkillLog @skillId={{value}}/>
        {{else}}
          {{value}}
        {{/if}}
      </r.cell>
    </b.row>
  </t.body>
</EmberTable>
