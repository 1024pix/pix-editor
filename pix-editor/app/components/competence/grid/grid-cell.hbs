{{#if (eq this.cellType "production")}}
  <Competence::Grid::CellProduction @skill={{@skill}} @link={{@link}} @languageFilter={{@languageFilter}}/>
{{else if (eq this.cellType "workbench")}}
  <Competence::Grid::CellWorkbench @tube={{@tube}} @skill={{@skill}} @skills={{@skills}}/>
{{else if (eq this.cellType "skill-workbench")}}
  <Competence::Grid::CellSkillWorkbench @tube={{@tube}} @skill={{@skill}} @skills={{@skills}}/>
{{else if (eq this.cellType "skill")}}
  <Competence::Grid::CellSkill @skill={{@skill}} @languageFilter={{@languageFilter}}/>
{{else if (eq this.cellType "skill-draft")}}
  <Competence::Grid::CellSkill @skill={{@skill}} @languageFilter={{@languageFilter}}/>
{{else if (eq this.cellType "add-skill")}}
  <td>
    <LinkTo @route="competence.skills.new" @models={{array @tube.id @index}} class="add-skill">
    <i class="icon plus circle"></i></LinkTo>
  </td>
{{else if (eq this.cellType "quality")}}
  <Competence::Grid::CellQuality @skill={{@skill}} />
{{else if (eq this.cellType "i18n")}}
  <Competence::Grid::CellI18n @skill={{@skill}} />
{{else}}
  <td class="skill-cell__empty">
    <div class="no-visibility">{{@tube.name}}</div>
  </td>
{{/if}}
