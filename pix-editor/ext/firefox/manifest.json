{

    "manifest_version": 2,
    "name": "PIX Editor",
    "description": "Affiche une épreuve directement dans PIX Editor à partir d'une URL",
    "version": "2.3",

    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Pix Editor"
    },

    "background": {
      "scripts": ["script.js"]
    },

    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ]

  }