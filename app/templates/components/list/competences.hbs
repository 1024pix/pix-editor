<div class='{{this.listType}}'>
  <EmberTable as |t|>
    <t.head @columns={{this.headers}} @widthConstraint={{'eq-container'}} @resizeMode={{'fluid'}} @onUpdateSorts={{this.testSort}} @sorts={{this.sorts}}/>
    <t.body @rows={{this.sortedList}} @rowSelectionMode={{'single'}} @checkboxSelectionMode={{'none'}} @onSelect={{this.selectRow}} @selection={{this.current}}/>
  </EmberTable>
</div>
