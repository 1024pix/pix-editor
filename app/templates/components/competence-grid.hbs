<table class="ui celled table definition tubes">
  <tbody>
  {{#each tubes as |tube|}}
  <tr>
    <td>{{tube.name}}</td>
    {{#each tube.filledSkills as |skill|}}
      {{#if (and skill.name (or skillMode (not production)  skill.productionTemplate))}}
          {{competence-grid-skill skill=skill skillMode=skillMode production=production link=link competenceModel=competenceModel}}
      {{else}}
        <td></td>
      {{/if}}
    {{/each}}
  </tr>
  {{/each}}
  </tbody>
</table>
